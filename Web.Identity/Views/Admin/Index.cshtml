@model List<AppUser>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
@if (Model.Count > 0)
{
    <table class="table table-bordered table-striped">
        <tr>
            <td>User Id</td>
            <td>User Name</td>
            <td>Email</td>
        </tr>

        @foreach (AppUser user in Model)
        {
            <tr>
                <td>@user.Id</td>
        <td>@user.UserName</td>
        <td>@user.Email</td>
    </tr>
        }
    </table>}
    else{
        <h3>Üye Bulunamadı!</h3>
    }